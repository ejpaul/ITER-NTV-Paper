@article{Hirshman1986a,
abstract = {A variational principle is developed for computing accurate values of local plasma transport coefficients in nonsymmetric toroidal confinement configurations. Numerical solutions of the linearized drift Fokker–Planck equation are used to obtain the thermodynamic fluxes as functions of collision frequency and the radial electric field. Effects resulting from the variation of the longitudinal adiabatic invariant J along an orbit (resulting from particle transitions from helically trapped to toroidally trapped orbits) are treated. The velocity‐space distribution resulting from trapped, circulating, and transition particle orbits is well represented by a Legendre polynomial expansion in the pitch angle coordinate. The computational effort is significantly reduced from that required with Monte Carlo methods through use of an efficient treatment of the disparity between the time scales of collisionless and collisional particle dynamics. Numerical computations for a stellarator configuration are presented.},
author = {Hirshman, S. P. and Shaing, K. C. and van Rij, W. I. and Beasley, C. O. and Crume, E. C.},
doi = {10.1063/1.865495},
file = {:Users/elizabethpaul/Documents/Research/Papers/Hirschman 1986 POF Plasma transport coefficients for nonsymmetric toroidal confinement systems.pdf:pdf},
issn = {0031-9171},
journal = {Physics of Fluids},
keywords = {ADIABATIC PROCESSES,CALCULATION METHODS,CHARGED−PARTICLE TRANSPORT,COLLISIONAL PLASMA,COLLISIONLESS PLASMA,ELECTRIC FIELDS,FOKKER−PLANCK EQUATION,HELICITY,KINETIC EQUATIONS,NUMERICAL SOLUTION,ORBITS,PLASMA,PLASMA CONFINEMENT,STELLARATORS,THERMODYNAMICS,TOROIDAL CONFIGURATION,TRAPPING,VARIATIONAL METHODS},
mendeley-groups = {Codes/DKES},
number = {9},
pages = {2951--2959},
title = {{Plasma transport coefficients for nonsymmetric toroidal confinement systems}},
url = {http://scitation.aip.org/content/aip/journal/pof1/29/9/10.1063/1.865495},
volume = {29},
year = {1986}
}

@article{Rij1989,
abstract = {A variational principle is developed for the linearized drift‐kinetic, Fokker–Planck equation, from which both upper and lower bounds for neoclassical transport coefficients can be calculated for plasmas in three‐dimensional toroidal confinement geometries. These bounds converge monotonically with the increasing phase‐space dimensionality of the assumed trial function. This property may be used to identify those portions of phase space that make dominant contributions to the transport process. A computer code based on this principle has been developed that uses Fourier–Legendre expansions for the poloidal, toroidal, and pitch‐angle dependences of the distribution function. Numerical calculations of transport coefficients for a plasma in the TJ‐II flexible heliac [Nucl. Fusion 2 8, 157 (1988)] are used to demonstrate the application of this procedure.},
author = {van Rij, W. I. and Hirshman, S. P.},
