@article{Garofalo2002,
abstract = {Values of the normalized plasma pressure up to twice the free-boundary stability limit predicted by ideal magnetohydrodynamic (MHD) theory have been sustained in the DIII-D tokamak. Long-wavelength modes are stabilized by the resistive wall and rapid plasma toroidal rotation. High rotation speed is maintained by minimization of nonaxisymmetric magnetic fields, overcoming a long-standing impediment [E. J. Strait et al., Phys. Rev. Lett. 74, 2483 (1995)]. The ideal-MHD pressure limit calculated with an ideal wall is observed as the operational limit to the normalized plasma pressure.},
author = {Garofalo, A. M. and Strait, E. J. and Johnson, L. C. and {La Haye}, R. J. and Lazarus, E. A. and Navratil, G. A. and Okabayashi, M. and Scoville, J. T. and Taylor, T. S. and Turnbull, A. D.},
