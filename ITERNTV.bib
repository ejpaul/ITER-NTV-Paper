mendeley-groups = {Rotation shear},
number = {9},
pages = {2940},
title = {{Rotation shear induced fluctuation decorrelation in a toroidal plasma}},
url = {http://scitation.aip.org/content/aip/journal/pop/1/9/10.1063/1.870534},
volume = {1},
year = {1994}
}

