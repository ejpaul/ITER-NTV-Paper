@article{Matsuoka2015,
abstract = {In evaluating neoclassical transport by radially-local simulations, the magnetic drift tangential to a flux surface is usually ignored in order to keep the phase-space volume conservation. In this paper, effect of the tangential magnetic drift on the local neoclassical transport are investigated. To retain the effect of the tangential magnetic drift in the local treatment of neoclassical transport, a new local formulation for the drift kinetic simulation is developed. The compressibility of the phase-space volume caused by the tangential magnetic drift is regarded as a source term for the drift kinetic equation, which is solved by using a two-weight {\$}\backslashdelta f{\$} Monte Carlo method for non-Hamiltonian system [G.{\~{}}Hu and J.{\~{}}A.{\~{}}Krommes, Phys. Plasmas {\$}\backslashrm \backslashtextbf{\{}1{\}}{\$}, 863 (1994)]. It is demonstrated that the effect of the drift is negligible for the neoclassical transport in tokamaks. In non-axisymmetric systems, however, the tangential magnetic drift substantially changes the dependence of the neoclassical transport on the radial electric field {\$}E{\_}{\{}\backslashrm r{\}}{\$}. The peaked behavior of the neoclassical radial fluxes around {\$}E{\_}{\{}\backslashrm r{\}} = 0{\$} observed in conventional local neoclassical transport simulations is removed by taking the tangential magnetic drift into account.},
author = {Matsuoka, Seikichi and Satake, Shinsuke and Kanno, Ryutaro and Sugama, Hideo},
file = {:Users/elizabethpaul/Documents/Research/Papers/Matuoka 2015 Effects of magnetic drift tangential to magnetic surfaces on neoclassical transport in non-axisymmetric plasmas.pdf:pdf},
issn = {10897674},
journal = {Physics of Plasmas},
mendeley-groups = {Magnetic Drifts},
number = {7},
title = {{Effects of magnetic drift tangential to magnetic surfaces on neoclassical transport in non-axisymmetric plasmas}},
url = {http://dx.doi.org/10.1063/1.4923434},
volume = {22},
year = {2015}
